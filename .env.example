# .env.example for Discord Quote & Song Bot

# Copy this file to .env and fill in your values

# All scheduled times are in EST (US/Eastern)

# Discord bot token

DISCORD\_TOKEN=

# Main server (guild) ID\GUILD\_ID=

# Channel IDs (all must be valid for your server)

QUOTE\_CHANNEL\_ID=
ICON\_CHANNEL\_ID=
POST\_CHANNEL\_ID=
MUSIC\_CHANNEL\_ID=
SONG\_POST\_CHANNEL\_ID=

# Feature toggles (true or false, not case-sensitive)

ENABLE\_DAILY\_QUOTE=true
ENABLE\_DAILY\_SONG=true

# Times for scheduled features (24h format, e.g. 4:00 for 4AM, 13:00 for 1PM)

# Always use EST

QUOTE\_TIME=4:00
SONG\_TIME=10:00
